{
  "project_type": "sagemaker-training-job",
  "version": "1.0.0",
  "description": "Template for SageMaker training job projects",
  "ci_cd_config": {
    "pipeline_type": "training",
    "stages": ["validate", "build", "train", "register", "package", "release"],
    "artifacts": [
      "training_script.py",
      "requirements.txt",
      "model_artifacts/",
      "training_output/",
      "deployment_config.json"
    ],
    "zip_naming": "candidate-PR-{PR_ID}-{COMMIT_ID}-{TIMESTAMP}.zip",
    "stable_zip_naming": "stable-{TIMESTAMP}.zip"
  },
  "sagemaker_config": {
    "framework": "pytorch",
    "instance_type": "ml.m5.large",
    "max_runtime": 3600,
    "hyperparameters": {
      "epochs": 10,
      "batch_size": 32,
      "learning_rate": 0.001
    }
  },
  "dependencies": [
    "torch",
    "torchvision",
    "numpy",
    "pandas",
    "scikit-learn",
    "boto3",
    "sagemaker"
  ],
  "file_structure": {
    "src/": "Source code directory",
    "src/training/": "Training scripts",
    "src/utils/": "Utility functions",
    "tests/": "Unit tests",
    "requirements.txt": "Python dependencies",
    "Dockerfile": "Container configuration",
    ".gitlab-ci.yml": "CI/CD pipeline configuration"
  }
}
